<form action="/user/bill/<%= products.id%>" method="post" enctype="multipart/form-data">

    <div class="destination_details_info">
        <div class="container">
            <div class="row justify-content-center">
                <div class="col-lg-8 col-md-9">

                    <div class="bordered_1px"></div>
                    <div class="contact_join">
                        <h3>Book Now</h3>
                        <form action="#">
                            <div class="row">
                              
                                <div class="col-lg-6 col-md-6">
                                    <div class="single_input">
                                       Code : <input type="text" placeholder="Your Name" id="code" name="code">
                                    </div>
                                </div>

                                <div class="col-lg-6 col-md-6">
                                    <div class="single_input">
                                       Name : <input type="text" placeholder="Your Name" id="name" name="name">
                                    </div>
                                </div>

                                <div class="col-lg-6 col-md-6">
                                    <div class="single_input">
                                       Sex : <input type="text" placeholder="Sex" id="sex" name="sex">
                                    </div>
                                </div>

                                <div class="col-lg-6 col-md-6">
                                    <div class="single_input">
                                       Phone: <input type="text" placeholder="Phone no." id="phone" name="phone">
                                    </div>
                                </div>

                                <div class="col-lg-6 col-md-6">
                                    <div class="single_input">
                                      Email:  <input type="text" placeholder="Email" id="email" name="email">
                                    </div>
                                </div>

                                <div class="col-lg-6 col-md-6">
                                    <div class="single_input">
                                        Number of People:   <input type="text" placeholder="Number of People" id="amountUser" name="amountUser">
                                    </div>
                                </div>

                                <div class="col-lg-6 col-md-6">
                                    <div class="single_input">
                                        Date Go:   <input type="date" placeholder="Number of People" id="datego" name="datego">
                                    </div>
                                </div>

                                <div class="col-lg-6 col-md-6">
                                    <div class="single_input">
                                        Date Return:   <input type="date" placeholder="Number of People" id="datereturn" name="datereturn">
                                    </div>
                                </div>
                        <h3>About Tour</h3>
                                <div class="col-lg-6 col-md-6">
                                    <div class="single_input">
                                        Name : <input type="text" placeholder="<%= products.name%>" disabled>
                                    </div>
                                </div>

                                <div class="col-lg-6 col-md-6">
                                    <div class="single_input">
                                      Price :  <input type="text" placeholder="<%= products.price%>" disabled>
                                    </div>
                                </div>

                                <div class="col-lg-6 col-md-6">
                                    <div class="single_input">
                                    Package :<input type="text" placeholder="<%= products.package%>" disabled>
                                    </div>
                                </div>


                                <div class="col-lg-12">
                                    <div class="submit_btn">
                                        <button class="boxed-btn4" type="submit">submit</button>
                                    </div>
                                </div>
                            </div>
                        </form>
                    </div>
                </div>
            </div>
        </div>
    </div>

</form>

